{
  "name": "puppet-profiles",
  "version": "0.3.0",
  "summary": "meta module which hold all profiles for the DCC thesis.",
  "author": "DCC",
  "dependencies": [
    {
      "name": "voxpupuli/grafana",
      "version_requirement": ">= 2.6.3 < 3.0.0"
    }
  ],
  "source": "https://github.com/DCC-org/puppet-profiles.git",
  "project_page": "https://github.com/DCC-org/puppet-profiles",
  "issues_url": "https://github.com/DCC-org/puppet-profiles/issues",
  "license": "Apache-2.0",
  "tags": [
    "monitoring"
  ],
